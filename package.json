{
  "name": "@jcoreio/mutate",
  "version": "0.0.0-development",
  "description": "a few immutable.js-like mutation functions for plain objects",
  "sideEffects": false,
  "config": {
    "commitizen": {
      "path": "cz-coventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/mutate.git"
  },
  "keywords": [
    "immutable",
    "deep",
    "json",
    "mutations",
    "update",
    "update-in",
    "set-in"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/mutate/issues"
  },
  "homepage": "https://github.com/jcoreio/mutate#readme",
  "devDependencies": {
    "@jedwards1211/js-toolchain": "^1.1.0",
    "flow-bin": "^0.135.0",
    "husky": "^4.3.0"
  },
  "dependencies": {
    "iterall": "^1.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "hooks": {
    "pre-commit": "toolchain pre-commit",
    "commit-msg": "toolchain commitlint -E HUSKY_GIT_PARAMS"
  },
  "scripts": {
    "test": "toolchain test",
    "prepublishOnly": "echo This package is meant to be published by semantic-release from the dist build directory. && exit 1",
    "tc": "toolchain",
    "toolchain": "toolchain"
  }
}
